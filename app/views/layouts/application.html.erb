<!DOCTYPE html>
<html>
  <head>
    <title>HexagonalConsultingTest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <%= render 'layouts/header' %>
    <% if notice %>
      <p class="notice alert alert-primary"><%= notice %></p>
    <% end %>
    <% if alert %>
      <p class="alert alert-warning"><%= alert %></p>
    <% end %>
    <%= yield %>
  </body>
</html>
